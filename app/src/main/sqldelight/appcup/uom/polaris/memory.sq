CREATE TABLE IF NOT EXISTS memory (
    id INTEGER PRIMARY KEY NOT NULL,
    latitude REAL NOT NULL,
    longitude REAL NOT NULL,
    path TEXT NOT NULL,
    journey_id TEXT
);

selectAll:
SELECT *
FROM memory;

insert:
INSERT INTO memory(latitude, longitude, path, journey_id)
VALUES (?,?,?,?);

delete:
DELETE FROM memory
WHERE id = ?;

selectMemories:
SELECT *
FROM memory
WHERE journey_id = ?;